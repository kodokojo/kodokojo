{
  "id": "$ID",
  "instance": 1,
  "cpus": 0.4,
  "mem": 256,
  "type": "DOCKER",
  "container": {
    "docker": {
      "image": "registry:2",
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 5000,
          "hostPort": 0,
          "protocol": "tcp",
          "servicePort": 0
        }
      ],
      "parameters": [

        {
          "key": "label",
          "value": "project=$projectName"
        },
        {
          "key": "label",
          "value": "componentType=repository"
        },
        {
          "key": "label",
          "value": "component=dockerregistry"
        }
      ]
    }
  },#if( $constrainByTypeAttribute )
  "constraints": [
    [
      "type",
      "LIKE",
      "operator"
    ]
  ],#end
  "healthChecks": [
    {
      "protocol": "HTTP",
      "portIndex": 0,
      "path": "/",
      "gracePeriodSeconds": 180,
      "intervalSeconds": 20,
      "maxConsecutiveFailures": 10
    }
  ],
  "labels": {
    "project": "$projectName",
    "componentType": "repository",
    "component": "dockerregistry"
  }
}